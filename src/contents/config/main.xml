<?xml version="1.0" encoding="UTF-8"?>
<kcfg xmlns="http://www.kde.org/standards/kcfg/1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.kde.org/standards/kcfg/1.0 http://www.kde.org/standards/kcfg/1.0/kcfg.xsd">
    <kcfgfile name=""/>
    <group name="">
        <entry name="restoreType" type="Int">
            <default>2</default>
        </entry>
        <entry name="restoreSize" type="Bool">
            <default>true</default>
        </entry>
        <entry name="restoreVirtualDesktop" type="Bool">
            <default>true</default>
        </entry>
        <entry name="moveVirtualDesktop" type="Bool">
            <default>false</default>
        </entry>
        <entry name="restoreActivities" type="Bool">
            <default>true</default>
        </entry>
        <entry name="moveActivity" type="Bool">
            <default>false</default>
        </entry>
        <entry name="restoreMinimized" type="Bool">
            <default>true</default>
        </entry>
        <entry name="restoreKeepAbove" type="Bool">
            <default>true</default>
        </entry>
        <entry name="restoreKeepBelow" type="Bool">
            <default>true</default>
        </entry>
        <entry name="restoreWindowsWithoutCaption" type="Bool">
            <default>true</default>
        </entry>
        <entry name="restoreTile" type="Bool">
            <default>true</default>
        </entry>
        <entry name="restoreResizedQuickTile" type="Bool">
            <default>false</default>
        </entry>
        <entry name="minimumCaptionMatch" type="Int">
            <default>0</default>
            <min>0</min>
            <max>100</max>
        </entry>
        <entry name="multiMonitorType" type="Int">
            <default>0</default>
        </entry>
        <entry name="printType" type="Int">
            <default>0</default>
        </entry>
        <entry name="instantRestore" type="Bool">
            <default>true</default>
        </entry>
        <entry name="multiWindowRestoreAttempts" type="Int">
            <default>5</default>
            <min>1</min>
            <max>60</max>
        </entry>
        <entry name="usePerfectMultiWindowRestore" type="Bool">
            <default>true</default>
        </entry>
        <entry name="perfectMultiWindowRestoreAttempts" type="Int">
            <default>12</default>
            <min>1</min>
            <max>60</max>
        </entry>
        <entry name="perfectMultiWindowRestoreList" type="String">
            <default>brave-browser
org.mozilla.firefox
firefox
vivaldi-stable
librewolf
chromium-browser
Chromium-browser
google-chrome
microsoft-edge
Mullvad Browser
Opera
io.github.ungoogled_software.ungoogled_chromium
app.zen_browser.zen
waterfox-default</default>
        </entry>
        <entry name="loginBoost" type="Bool">
            <default>true</default>
        </entry>
        <entry name="loginBoostMultiplier" type="Int">
            <default>2</default>
            <min>2</min>
            <max>8</max>
        </entry>
        <entry name="sessionRestore" type="Bool">
            <default>false</default>
        </entry>
        <entry name="sessionRestoreSize" type="Bool">
            <default>true</default>
        </entry>
        <entry name="sessionRestoreVirtualDesktop" type="Bool">
            <default>true</default>
        </entry>
        <entry name="sessionRestoreActivities" type="Bool">
            <default>true</default>
        </entry>
        <entry name="sessionRestoreMinimized" type="Bool">
            <default>true</default>
        </entry>
        <entry name="sessionRestoreKeepAbove" type="Bool">
            <default>true</default>
        </entry>
        <entry name="sessionRestoreKeepBelow" type="Bool">
            <default>true</default>
        </entry>
        <entry name="sessionRestoreTime" type="Int">
            <default>25</default>
            <min>15</min>
            <max>120</max>
        </entry>
        <entry name="blacklist" type="String">
            <default>org.kde.spectacle
org.kde.polkit-kde-authentication-agent-1
steam*
org.kde.plasmashell
kwin
ksmserver
systemsettings</default>
        </entry>
        <entry name="whitelist" type="String">
            <default>brave-browser
org.mozilla.firefox
firefox
vivaldi-stable
librewolf
chromium-browser
Chromium-browser
google-chrome
microsoft-edge
Mullvad Browser
Opera
io.github.ungoogled_software.ungoogled_chromium
app.zen_browser.zen
waterfox-default</default>
        </entry>
        <entry name="printApplicationNameToLog" type="Bool">
            <default>true</default>
        </entry>
        <entry name="ignoreNumbers" type="Bool">
            <default>true</default>
        </entry>
        <entry name="onlySaveOnShutdown" type="Bool">
            <default>false</default>
        </entry>
        <entry name="debugLogs" type="Bool">
            <default>false</default>
        </entry>
        <entry name="printMonitorInfoToLog" type="Bool">
            <default>false</default>
        </entry>
    </group>
</kcfg>
